// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int vowel(char a[],int n)
{
    int count=0;
    for(int i=0;i<n;i++)
    {
        if((a[i]=='a')||(a[i]=='e')||(a[i]=='i')||(a[i]=='o')||(a[i]=='u'))
        {
            count++;
        }
    }
    return count;
    
    
}
int main() {
    
    
     char a[]={'a','b','c','d','e'};
         int n=sizeof(a)/sizeof(a[0]);
         cout<<vowel(a,n);
         

    return 0;
}
